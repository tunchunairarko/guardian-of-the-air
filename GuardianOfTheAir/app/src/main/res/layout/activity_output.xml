<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.mohammodraihanulbash.guardianoftheair.OutputActivity"
    >


    <ImageView
        android:layout_width="140dp"
        android:layout_height="140dp"

        android:id="@+id/imageView2"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="45dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Thank You "
        android:textStyle="bold"
        android:textSize="50dp"
        android:textColor="#000"
        android:shadowColor="#009900"
        android:id="@+id/textView28"
        android:layout_below="@+id/imageView2"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Guardian [name]"
        android:textSize="30dp"
        android:gravity="center"
        android:paddingLeft="15dp"
        android:id="@+id/textView29"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/textView28"
        android:layout_alignStart="@+id/textView28" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="You received [10] points!"
        android:id="@+id/textView30"
        android:textSize="20dp"
        android:textStyle="bold"
        android:textColor="#000"
        android:paddingTop="10dp"
        android:layout_below="@+id/textView29"
        android:layout_alignRight="@+id/textView29"
        android:layout_alignEnd="@+id/textView29" />

    <Button
        android:layout_width="150dp"
        android:layout_height="80dp"
        android:paddingTop="20dp"
        android:text="See Your Ranking"
        android:id="@+id/button13"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_below="@+id/textView30"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="150dp"
        android:layout_height="80dp"
        android:text="See how you contributed"
        android:textSize="20dp"
        android:textStyle="bold"
        android:id="@+id/button14"
        android:layout_below="@+id/button13"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/button13"
        android:layout_alignEnd="@+id/button13" />

    <Button
        android:layout_width="150dp"
        android:layout_height="80dp"
        android:text="Back to guardian menu"
        android:textSize="20dp"
        android:textStyle="bold"
        android:id="@+id/button15"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/button14"
        android:layout_alignEnd="@+id/button14" />


</RelativeLayout>
